{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Fonte de dados Prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "9.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": "9.0.0"
    },
    {
      "type": "panel",
      "id": "heatmap",
      "name": "Heatmap",
      "version": "9.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Block Height",
      "id": 1,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        { "expr": "max(block_height)", "legendFormat": "height", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short", "decimals": 0 },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Peer Count",
      "id": 2,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        { "expr": "max(peer_count)", "legendFormat": "peers", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short", "decimals": 0 },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Avg Block Processing Time (s)",
      "id": 3,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        { "expr": "avg(avg_block_processing_time)", "legendFormat": "avg_s", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s", "decimals": 2 },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Memory Usage (GB)",
      "id": 4,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 0 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        { "expr": "max(memory_usage_gigabytes)", "legendFormat": "mem_gb", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "decgbytes", "decimals": 2 },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Block Height over time",
      "id": 5,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        { "expr": "block_height", "legendFormat": "{{instance}}", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Peers over time",
      "id": 6,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        { "expr": "peer_count", "legendFormat": "{{instance}}", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Message latency quantiles (s)",
      "id": 7,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 14 },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 2 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (rate(message_times_bucket[5m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.9, sum by (le) (rate(message_times_bucket[5m])))",
          "legendFormat": "p90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(message_times_bucket[5m])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Message rate (responses/s)",
      "id": 8,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 14 },
      "fieldConfig": { "defaults": { "unit": "reqps", "decimals": 2 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        { "expr": "sum(rate(message_times_count[5m]))", "legendFormat": "rate", "refId": "A" }
      ]
    },
    {
      "type": "heatmap",
      "title": "Message latency heatmap (s)",
      "id": 9,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 23 },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "options": {
        "legend": { "show": true },
        "tooltip": { "show": true },
        "yAxis": { "decimals": 2, "format": "s", "logBase": 1 },
        "color": { "mode": "spectrum" }
      },
      "targets": [
        { "expr": "sum(rate(message_times_bucket[5m])) by (le)", "format": "heatmap", "legendFormat": "{{le}}", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Avg block processing time (s)",
      "id": 10,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 33 },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 2 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        { "expr": "avg_block_processing_time", "legendFormat": "{{instance}}", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "System memory usage (GB)",
      "id": 11,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 33 },
      "fieldConfig": { "defaults": { "unit": "decgbytes", "decimals": 2 }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        { "expr": "memory_usage_gigabytes", "legendFormat": "{{instance}}", "refId": "A" }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["floresta", "bitcoin", "utreexo", "node"],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 2,
        "hide": 2,
        "query": "label_values(block_height, job)",
        "includeAll": false,
        "multi": false,
        "current": { "selected": true, "text": "prometheus", "value": "prometheus" }
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 2,
        "query": "label_values(block_height, instance)",
        "includeAll": true,
        "multi": true,
        "current": { "selected": true, "text": ["All"], "value": ["$__all"] }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "Floresta Node Metrics",
  "uid": "floresta-node",
  "version": 1
}

